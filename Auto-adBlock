  auto-detect-interface: true
  dns-hijack:
    - 114.114.114.114
    - 223.5.5.5
    - 8.8.8.8
    - 1.1.1.1

# Proxy Providers
proxy-providers:
  provider:
    type: http
    url: https://drive.google.com/uc?export=download&id=1rKIaOSbfqoD4FEUnn1CA4KtFnrIzocs0
    interval: 3600
    path: ./providers/provider.yaml
    health-check:
      enable: true
      interval: 300
      url: http://www.gstatic.com/generate_204

# Proxy Groups
proxy-groups:
  - name: "☁️ Proxy"
    type: select
    proxies:
      - ⚡ Auto
      - ✏️ Select


  - name: "✏️ Select"
    type: select
    proxies:
    use:
      - provider
  - name: "⚡ Auto"
    type: url-test
    url: http://www.gstatic.com/generate_204
    interval: 300
    proxies:
    use:
      - provider

  - name: "🤖 AI Services"
    type: select
    proxies:
      - "⚡ Auto"
      - "✏️ Select"
      - DIRECT

  - name: "🛡️ AdBlock"
    type: select
    proxies:
      - REJECT
      - DIRECT

  - name: "🇲🇲 Myanmar Sites"
    type: select
    proxies:
      - DIRECT
      - "⚡ Auto"
      - "✏️ Select"

  - name: "🌍 Global Sites"
    type: select
    proxies:
      - "⚡ Auto"
      - "✏️ Select"
      - DIRECT

  - name: "🎮 Gaming"
    type: select
    proxies:
      - DIRECT
