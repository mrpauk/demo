  - name: "🎬 Streaming"
    type: select
    proxies:
      - "⚡ Auto"
      - "✏️ Select"
      - DIRECT

# Rule Providers
rule-providers:
  reject:
    type: http
    behavior: domain
    url: https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/reject.txt
    path: ./ruleset/reject.yaml
    interval: 86400

