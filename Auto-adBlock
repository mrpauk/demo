proxy-groups:
  - name: 🚀 Proxy
    type: select
    proxies:
      - ♻️ Auto
      - DIRECT
    use:
      - provider1
      - provider2
      - provider3
      - provider4
      - provider5

  - name: ♻️ Auto
    type: url-test
    use:
      - provider1
      - provider2
      - provider3
      - provider4
      - provider5
    url: http://www.gstatic.com/generate_204
    interval: 300

  - name: 🇲🇲 Myanmar
    type: select
    proxies:
      - DIRECT

  - name: 🌍 Global
    type: select
    proxies:
      - 🚀 Proxy
      - ♻️ Auto
      - DIRECT
      
      
  - name: 🚧 AdBlock
    type: select
    proxies: 
      - REJECT
      - DIRECT
      
rule-providers:
  adblock:
    type: http
    behavior: domain
    url: "https://anti-ad.net/clash.yaml"
    path: ./rules/adblock.yaml
    interval: 86400     


