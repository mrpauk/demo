# DNS settings for better stability
dns:
  enable: true
  listen: 0.0.0.0:53
  enhanced-mode: fake-ip
nameserver:
  - https://dns.alidns.com/dns-query  
  - tls://223.5.5.5:853              
  - 223.5.5.5                        
  - 114.114.114.114                
  - https://1.1.1.1/dns-query        
